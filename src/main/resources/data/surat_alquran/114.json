        "2": {
            "teksArab": "مَلِكِ النَّاسِۙ",
            "teksLatin": "malikin-nās(i).",
            "teksIndonesia": "Raja manusia,"
        },
        "3": {
            "teksArab": "اِلٰهِ النَّاسِۙ",
            "teksLatin": "ilāhin-nās(i).",
            "teksIndonesia": "sembahan manusia,"
